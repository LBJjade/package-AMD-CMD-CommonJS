{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error",
                    "log"
                ]
            }
        ],
        "no-extra-boolean-cast": 0
    },
    "globals": {
      "$": true,
      "Picker": true,
      "IosSelect": true
    }
}